{
  "title": "石器时代客户端脚本接口",
  "author": "libla",
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "ga",
    "github",
    "-highlight",
    "prism",
    "bibtex-indexed-cite",
    "anchor-navigation-ex",
    "page-footer-ex",
    "splitter",
    "todo",
    "copy-code-button",
    "expandable-chapters-small"
  ],
  "pluginsConfig": {
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord" : ["Gitbook Use"]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "ga": {
      "token": "UA-131910384-3"
    },
    "github": {
      "url": "https://github.com/libla/feilu-clientscript"
    },
    "prism": {
      "ignore": [
        "mermaid",
        "eval-js"
      ],
      "css": [
        "prismjs/themes/prism-solarizedlight.css"
      ]
    },
    "page-footer-ex": {
      "copyright": "By [libla](https://github.com/libla)",
      "markdown": true,
      "update_label": "此页面修订于：",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },
    "bibtex-indexed-cite": {
      "path": "/"
    }
  }
}